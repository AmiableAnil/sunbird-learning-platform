                    "dims":{"x": 8,"y": 10,"w": 90,"h": 65, "pad": 8}
                }]
            }, {
                "type": "asset",
                "assetType": "image",
                "assetId": "bEqualsImg",
                "dims":{"x": 51,"y": 20,"w": 8,"h": 60}
            }, {
                "type": "container",
                "assetType": "image",
                "assetId": "5A_bg",
                "dims":{"x": 30,"y": 0,"w": 20,"h": 100},
                "displayObjects": [{
                    "type": "placeholder",
                    "placeholderId": "variable2",
                    "dims":{"x": 4,"y": 4,"w": 73,"h": 65}
