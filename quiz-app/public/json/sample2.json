            }]
        }, {
            "type": "placeholder",
            "placeholderId": "titleHeader",
            "dims":{"x": 5,"y": 10,"w": 60,"h": 10}
        }]
    },
    "placeholders": {
        "titleHeader": {
            "type": "asset",
            "assetType": "text",
            "assetValue": "Hello, Can you fill up the empty box with apples (either green or red) from the bottom box?",
            "color": "white",
            "font": "20px solid Arial"
        },
        "variable1": {
            "type": "gridLayout",
            "assets": [{"id":"appleG","repeat": 5}, {"id":"banana","repeat": 9}]
        },
        "variable2": {
            "type": "asset",
            "assetType": "input",
            "fontSize": "100px",
            "fontFamily": "Arial",
            "fontColor": "#212121",
            "fontWeight": "bold"
        },
        "variable3": {
            "type": "gridLayout",
            "assets": [{"id":"appleG","repeat": 5}, {"id":"banana","repeat": 6}, {"id":"appleG","repeat": 7}, {"id":"apple","repeat": 8}]
        },
        "variable4": {
            "type": "gridLayout",
            "assets": [{"id":"appleG","repeat": 20}, {"id":"banana","repeat": 10}, {"id":"apple","repeat": 20}],
            "enableDrag": true,
            "snapTo": "variable2"
        }
    }
