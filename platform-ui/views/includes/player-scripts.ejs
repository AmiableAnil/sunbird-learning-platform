<script type="text/javascript">
    $(function(){
        $('.textarea').wysihtml5();
        $('#WriteNote').on('click',function() {
            $(".des_container").hide('slide');
        });
        $('#WriteNote').on('click',function() {
            $(".textEditor").show('slide');
        });
        $( ".dropdown-menu .menu" ).click(function( event ) {
            event.stopPropagation()
        });
        $("#collapseTxt").click(function(event){
            $("#il-Txt-Editor").slideToggle();
        });
    });

    $(document).ready(function() {
        $('.sideicon3').show();
        setTimeout(function() {
            //$('.number').numeric();
            $('.number').focusout(function() {
                if(this.value > 10 || this.value < 0) {
                    this.value = 0;
                } else {
                    this.value = parseInt(this.value);
                }
            });
        },1000);
    });

    function navigateTo(path, param) {
        document.location.href = path + encodeURIComponent(param);
    }

    function rightBarScrollWidth() {
        if ($('.enscroll-track')) {
            var scrollWidth = $('.enscroll-track').parent().offset().left;
            $('.enscroll-track').parent().css('left', scrollWidth-1);
        }
    }

    function windowHeight1() {
        var windowH = $(window).outerHeight();
        var headerH = $("header").outerHeight();
        var footerH = $(".footerClass").outerHeight();
        $(".RightSideBar").height(windowH - headerH);
        $(".mid-container-full").height(windowH - headerH);
        $(".mid-container").height(windowH - headerH - footerH);
    }

    setTimeout(function() {
        windowHeight1();
    }, 500);

</script>