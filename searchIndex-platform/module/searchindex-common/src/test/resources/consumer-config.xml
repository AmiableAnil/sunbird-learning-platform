<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ConsumerConfig>
	<ConsumerInit serverURI="localhost:9092" topic="composite-search-new"
		ekstepPlatformURI="http://localhost:8083/taxonomy-service"
		ekstepPlatformApiUserId="ilimi">
	</ConsumerInit>
	<ConsumerGroups>
		<ConsumerGroup id="composite-search-group">
			<Consumers>
				<Consumer
					messageProcessor="org.ekstep.searchindex.processor.CompositeSearchMessageProcessor"
					partitions="0,1,2,3,4,5" />
				<Consumer
					messageProcessor="org.ekstep.searchindex.processor.CompositeSearchMessageProcessor"
					partitions="6,7,8,9,10" />
			</Consumers>
		</ConsumerGroup>
		<ConsumerGroup id="word-count-group">
			<Consumers>
				<Consumer
					messageProcessor="org.ekstep.searchindex.processor.WordCountMessageProcessor"
					partitions="8" />
			</Consumers>
		</ConsumerGroup>
	</ConsumerGroups>
</ConsumerConfig>